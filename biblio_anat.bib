journal = {Biomed. Opt. Express},
keywords = {Medical and biological imaging; Multiphoton processes; Aberration compensation; Nonlinear microscopy; Active or adaptive optics; Confocal laser scanning microscopy; Deformable mirrors; Fluorescence microscopy; Imaging systems; Optical elements; Spatial light modulators},
number = {11},
