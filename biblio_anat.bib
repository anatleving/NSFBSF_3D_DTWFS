@article{Dean2003,
	author = {Bruce H. Dean and Charles W. Bowers},
	journal = {J. Opt. Soc. Am. A},
	keywords = {Phase retrieval; Talbot and self-imaging effects; Phase measurement; Deformable mirrors; Fourier transforms; Phase retrieval; Phase shift; Spatial frequency; Wave front sensing},
	number = {8},
	pages = {1490--1504},
	publisher = {Optica Publishing Group},
	title = {Diversity selection for phase-diverse phase retrieval},
	volume = {20},
	month = {Aug},
	year = {2003},
	url = {https://opg.optica.org/josaa/abstract.cfm?URI=josaa-20-8-1490},
	doi = {10.1364/JOSAA.20.001490},
	abstract = {Wavefront-sensing performance is assessed for focus-diverse phase retrieval as the aberration spatial frequency and the diversity defocus are varied. The analysis includes analytical predictions for optimal diversity values corresponding to the recovery of a dominant spatial-frequency component in the pupil. The calculation is shown to be consistent with the Cram\'{e}r--Rao lower bound by considering a sensitivity analysis of the point-spread function to the spatial frequency being estimated. A maximum value of diversity defocus is also calculated, beyond which wavefront-sensing performance decreases as diversity defocus is increased. The results are shown to be consistent with the Talbot imaging phenomena, explaining multiple periodic regions of maximum and minimum contrast as a function of aberration spatial frequency and defocus. Wavefront-sensing performance for an iterative-transform phase-retrieval algorithm is also considered as diversity defocus and aberration spatial frequency are varied.},
}

